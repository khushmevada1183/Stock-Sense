{
  "name": "indian-stock-sense-frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://khushmevada5005.github.io/stock-sense-build/",
  "scripts": {
    "dev": "next dev",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=4096 next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next",
    "clean:all": "rm -rf .next node_modules package-lock.json && npm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit cypress/unit",
    "test:api": "jest tests/api",
    "test:api-infra": "node tests/run-api-tests.js",
    "test:api-infra:indian": "node tests/run-api-tests.js --no-standard",
    "test:api-infra:standard": "node tests/run-api-tests.js --no-indian",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "next build && next start & cypress open",
    "e2e:headless": "next build && next start & cypress run",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "test:e2e:report": "cypress run --reporter cypress-mochawesome-reporter",
    "generate:bug-report": "node tests/utils/scripts/generate-bug-report.js",
    "export": "next export || echo 'Export included in build with Next.js 13+'",
    "build:static": "cross-env NODE_OPTIONS=--max_old_space_size=1024 next build && next export",
    "predeploy": "npm run build:static",
    "deploy": "gh-pages -d out"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "axios": "^1.6.2",
    "chart.js": "^4.4.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^12.12.1",
    "gsap": "^3.12.2",
    "lucide-react": "^0.292.0",
    "next": "^15.3.2",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-syntax-highlighter": "^15.6.1",
    "react-table": "^7.8.0",
    "recharts": "^2.10.1",
    "swiper": "^11.0.5",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/babel__core": "^7.20.1",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__template": "^7.4.1",
    "@types/babel__traverse": "^7.20.1",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-syntax-highlighter": "^15.5.7",
    "@types/react-table": "^7.7.18",
    "@types/sinonjs__fake-timers": "^8.1.2",
    "@types/sizzle": "^2.3.5",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.5.1",
    "cypress-mochawesome-reporter": "^3.7.0",
    "esbuild": "^0.25.4",
    "eslint": "^8.53.0",
    "eslint-config-next": "^15.3.2",
    "gh-pages": "^6.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2"
  }
}
